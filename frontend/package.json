{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "update:db": "npx prisma generate && npx prisma db push",
    "env:local": "rm -rf .env && rm -rf .env.local && cp .env.local_secrets .env.local && cp .env.local_secrets .env",
    "env:dev": "rm -rf .env && rm -rf .env.local && cp .env.development_secrets .env && cp .env.development_secrets .env.local",
    "local": "yarn env:local && yarn update:db && next dev --turbo",
    "dev": "yarn env:dev && yarn update:db && next dev --turbo"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.4.0",
