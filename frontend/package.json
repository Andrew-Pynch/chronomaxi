    "@tanstack/react-query": "^5.25.0",
    "@trpc/client": "next",
    "@trpc/react-query": "next",
    "@trpc/server": "next",
    "@types/d3": "^7.4.3",
    "@types/react-icons": "^3.0.0",
    "@types/react-toastify": "^4.1.0",
    "@types/recharts": "^1.8.29",
    "@types/uuid": "^9.0.8",
    "d3": "^7.9.0",
    "micro": "^10.0.1",
    "next": "^14.2.1",
    "next-auth": "^4.24.6",
    "next-themes": "^0.3.0",
    "react": "18.2.0",
    "react-countdown-circle-timer": "^3.2.1",
    "react-dom": "18.2.0",
    "react-icons": "^5.2.1",
    "react-toastify": "^10.0.5",
    "recharts": "^2.12.7",
    "server-only": "^0.0.1",
    "stripe": "^15.10.0",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.3.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.3",
    "zustand": "^4.5.2"
